{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"All My GW2 Stuff",
		"version":"0.0.2",
		"minimum-overwolf-version":"0.77.10",
		"author":"Blackshell",
		"icon":"IconMouseOver.png",
		"icon_gray":"IconMouseNormal.png",
		"description":"Demo App"
	},
	"data": {
		"start_window":"MainWindow",
		"windows":{
			"MainWindow":{
				"file":"Files/index.html",
				"transparent":true,
				"resizable":true,
				"size":{"width":400, "height":300},
				"min_size":{"width":200, "height":200},
				"max_size":{"width":900, "height":800}
			},
			"ItemInfoWindow": {
				"file": "Files/item.html",
				"transparent": true,
				"resizable": true,
				"size": {"width": 300, "height": "400"}
			}
		}
	}
}
